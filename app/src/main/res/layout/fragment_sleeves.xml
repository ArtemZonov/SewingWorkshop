<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".tamplates.SleevesFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/color_of_fragments">

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <ImageView
                    android:id="@+id/imageView73"
                    android:layout_width="match_parent"
                    android:layout_height="418dp"
                    android:src="@drawable/photo3" />

                <ImageView
                    android:id="@+id/imageView74"
                    android:layout_width="match_parent"
                    android:layout_height="414dp"
                    android:scaleType="fitXY"
                    android:src="@drawable/photo4" />

                <ImageView
                    android:id="@+id/imageView75"
                    android:layout_width="match_parent"
                    android:layout_height="272dp"
                    android:src="@drawable/photo6" />

                <ImageView
                    android:id="@+id/imageView76"
                    android:layout_width="match_parent"
                    android:layout_height="405dp"
                    android:src="@drawable/photo23" />

                <ImageView
                    android:id="@+id/imageView77"
                    android:layout_width="match_parent"
                    android:layout_height="454dp"
                    android:src="@drawable/photo29" />
            </LinearLayout>
        </androidx.core.widget.NestedScrollView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>