        <activity
            android:name=".manual.FormuliActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".manual.MerkiActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".manual.DefektiActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".manual.PribavkiActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".manual.ManualActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>