        <activity
            android:name=".clients.EditActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".clients.ClientsActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".manual.RashodTkaneiActivity"
